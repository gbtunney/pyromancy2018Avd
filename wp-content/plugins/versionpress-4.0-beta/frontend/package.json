  "license": "GPL",
  "main": "build/app.js",
  "version": "1.0.0",
  "config": {
    "loglevel": "warn"
  },
  "scripts": {
    "start": "node webpack/webpack-dev-server.js --config webpack/webpack.dev.js --env.port 8888",
    "build": "npm install && tsc --noEmit && webpack -p --config webpack/webpack.prod.js --hide-modules",
    "tsc": "tsc --noEmit",
    "clean": "rimraf build/* && git clean -fx -- \"src/**.js\" \"src/**.map\"",
    "lint": "tslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "core-js": "~2.4.1",
    "diff": "^3.2.0",
    "lodash": "^4.17.4",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.6",
    "moment": "^2.18.1",
    "react": "15.4.2",
    "react-day-picker": "^5.5.1",
    "react-dom": "15.4.2",
    "react-router": "^2.6.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/classnames": "^0.0.32",
    "@types/core-js": "^0.9.41",
    "@types/diff": "^3.2.0",
    "@types/history": "^4.5.1",
    "@types/lodash": "~4.14.37",
    "@types/mocha": "~2.2.32",
    "@types/node": "^7.0.18",
    "@types/react": "^15.0.13",
    "@types/react-day-picker": "^5.2.1",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^2.0.38",
    "@types/superagent": "~2.0.34",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "~3.5.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "less": "~2.7.1",
    "less-loader": "^4.0.3",
    "mocha": "^3.3.0",
    "postcss": "^5.2.15",
    "postcss-loader": "^1.3.2",
    "react-hot-loader": "~1.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.0.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.2",
